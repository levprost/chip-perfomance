{% extends 'base.html.twig' %}

{% block title %}User{% endblock %}

{% block body %}
    <h1>User</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Email</th>
                <td>{{ user.email }}</td>
            </tr>
            <tr>
                <th>First_name</th>
                <td>{{ user.firstName }}</td>
            </tr>
            <tr>
                <th>Last_name</th>
                <td>{{ user.lastName }}</td>
            </tr>
            <tr>
                <th>File</th>
                <td>{{ user.file }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_user_default_index') }}">back to list</a>

    <a href="{{ path('app_user_default_edit', {'id': user.id}) }}">edit</a>

    {{ include('user_default/_delete_form.html.twig') }}
{% endblock %}
